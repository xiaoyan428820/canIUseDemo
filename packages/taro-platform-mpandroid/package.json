{
  "name": "@tarojs/plugin-platform-mpandroid",
  "version": "3.6.8",
  "description": "Android 端平台插件",
  "author": "ZakaryCode",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "taro"
  ],
  "files": [
    "dist",
    "index.js",
    "types"
  ],
  "sideEffects": [
    "./dist/runtime/index.js",
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "lint": "eslint src --ext .js --ext .ts --ext .tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" && eslint src --ext .js --ext .jsx --ext .ts --ext .tsx --fix"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "dependencies": {
    "@tarojs/service": "workspace:*",
    "@tarojs/components": "workspace:*",
    "@tarojs/components-react": "workspace:*",
    "@tarojs/router": "workspace:*",
    "@tarojs/shared": "workspace:*",
    "@tarojs/taro-h5": "workspace:*",
    "babel-plugin-transform-taroapi": "workspace:*",
    "resolve": "^1.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "lodash": "^4.17.21",
    "rollup": "^4.9.6",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
